---
const { rotation, revolution, radius, temperature } = Astro.props;
import FooterCard from "./FooterCard.astro";
---

<footer>
  <FooterCard label="Rotation Time" value={rotation} />
  <FooterCard label="Revolution Time" value={revolution} />
  <FooterCard label="Radius:" value={radius} />
  <FooterCard label="Average Temp." value={temperature} />
</footer>
